---
title: blog
order: 2
---
<ul>
  {% for post in collections.posts %}
  <li>
    <article>
      <h3>
        <a href="{{ post.url }}">{{ post.data.title }}</a>
      </h3>
      <time datetime="{{ post.date | isoDate }}">{{ post.date | humanDate }}</time>
      <p>{{ post.data.excerpt | safe }}</p>
    </article>
  </li>
  {% endfor %}
</ul>